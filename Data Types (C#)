using System;

namespace Ourcode
{
	class program
	{
		static void Main(string[]args)
		{
			Console.WriteLine("Size of int: {0}", sizeof(int));
			Console.WriteLine("Size of float: {0}", sizeof(float));
			Console.WriteLine("Size of char: {0}", sizeof(char));
			Console.ReadKey();		
		}		
	}
}





