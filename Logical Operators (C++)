#include <iostream>

using namespace std;

int main()
{
    int a = 20;
    int b = 18;
    int c;



    if (a && b)
    {
        cout << "This is true." << endl;
    }
    else
    {
        cout << "This is false." << endl;
    }

    if (a || b)
    {
        cout << "This is true." << endl;
    }
    else
    {
        cout << "This is false." << endl;
    }

    if (!(a && b))
    {
        cout << "This is true." << endl;
    }
    else
    {
        cout << "This is false." << endl;
    }



    return 0;
}
